{
  "name": "vscode-freezeguard",
  "displayName": "VSCode FreezeGuard",
  "description": "Monitor and track VSCode UI freezes and performance",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": ["Other"],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "freezeguard.measure",
        "title": "Freeze Guard: Measure Current Action"
      },
      {
        "command": "freezeguard.badBlocking",
        "title": "Freeze Guard: Run BAD Blocking Action"
      },
      {
        "command": "freezeguard.backgroundFix",
        "title": "Freeze Guard: Run FIXED Background Action"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/node": "16.x",
    "typescript": "^4.9.4"
  }
}